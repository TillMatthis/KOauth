{
  "name": "@tillmatthis/koauth-client",
  "version": "0.1.0",
  "description": "Zero-dependency TypeScript client SDK for KOauth authentication",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "auth",
    "authentication",
    "koauth",
    "fastify",
    "express",
    "jwt",
    "session",
    "api-key"
  ],
  "author": "Till Matthis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TillMatthis/KOauth.git",
    "directory": "packages/koauth-client"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.9.0",
    "@types/supertest": "^6.0.3",
    "express": "^4.18.2",
    "fastify": "^4.28.1",
    "supertest": "^7.1.4",
    "tsup": "^8.0.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "peerDependencies": {
    "express": "^4.0.0 || ^5.0.0",
    "fastify": "^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "fastify": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
